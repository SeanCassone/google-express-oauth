{
  "name": "google-express-oauth",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "clean": "rm -rf ./build && mkdir build",
    "prebuild": "npm run clean",
    "build": "babel src -d ./build --ignore ./node_modules",
    "db:setup": "sequelize db:create",
    "db:migrate:up": "sequelize db:migrate",
    "db:migrate:down": "sequelize db:migrate:undo:all",
    "db:refresh": "npm run db:migrate:down && npm run db:migrate:up"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport-google-token": "^0.1.2",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@babel/runtime": "^7.15.3",
    "babel-eslint": "^10.1.0",
    "babel-node": "0.0.1-security",
    "nodemon": "^2.0.12",
    "sequelize-cli": "^6.2.0"
  }
}
